// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement())
  name    String?
  email   String   @unique
  pcid    Int?     @unique
}

model Layout {
  id      Int      @id @default(autoincrement())
  user_id Int
  name    String?
  slots   String?
}
